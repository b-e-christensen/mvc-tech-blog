    
    {{#each posts as |post|}}
    <div class="post-container">
        <div id="post-title" class="post-title display-flex justify-center">{{post.title}} posted by {{post.user.name}}</div>
        <div id="post-body" class="post-body col-12">{{post.content}}</div>
        {{!-- <div id="comment"></div> --}}
    </div>
    {{/each}}